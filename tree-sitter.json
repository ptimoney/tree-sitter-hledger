{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hledger",
      "camelcase": "hLedger",
      "title": "hledger",
      "scope": "source.hledger",
      "file-types": [
        "journal",
        "hledger"
      ],
      "injection-regex": "^hledger$",
      "class-name": "TreeSitterHledger"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "Tree-sitter grammar for hledger journal files",
    "authors": [
      {
        "name": "Patrick Timoney",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/ptimoney/tree-sitter-hledger",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}